{
  "name": "golden-hammer-services",
  "version": "1.0.0",
  "description": "My Moleculer-based microservices project",
  "author": "CodexHere <codexhere@outlook.com>",
  "license": "MIT",
  "type": "module",
  "prettier": "@hangouthere/util/prettier",
  "engines": {
    "node": "=18"
  },
  "scripts": {
    "start": "echo \"You should use the included hh-util Docker Compose scripts located in './node_modules/.bin/hh-compose*' to start this project\"",
    "_start": "moleculer-runner",
    "_cli": "moleculer connect"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "devDependencies": {
    "@hangouthere/util": "^0.0.1-4",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^28.1.1",
    "@types/tmi.js": "^1.8.1",
    "@types/uuid": "^8.3.4",
    "golden-hammer-shared": "^0.0.1-13",
    "moleculer-cli": "^0.8.1",
    "moleculer-repl": "^0.7.1"
  },
  "dependencies": {
    "ioredis": "^4.28.5",
    "jaeger-client": "^3.19.0",
    "moleculer": "^0.14.21",
    "moleculer-db": "^0.8.18",
    "moleculer-db-adapter-mongo": "^0.4.14",
    "moleculer-io": "^2.0.0",
    "moleculer-web": "^0.10.4",
    "nats": "^2.7.1",
    "tmi.js": "^1.8.5",
    "uuid": "^8.3.2"
  },
  "__NEEDS_PORTING_TOVITEST__jest": {
    "resetModules": true,
    "resetMocks": true,
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/$1"
    },
    "collectCoverageFrom": [
      "<rootDir>/services/**/*.{js,jsx}",
      "!<rootDir>/test/helpers/**",
      "!<rootDir>/node_modules/**"
    ]
  },
  "peerDependencies": {
    "golden-hammer-shared": "^0.0.1-1"
  }
}
