{
  "name": "golden-hammer-services",
  "version": "1.0.0",
  "description": "My Moleculer-based microservices project",
  "author": "CodexHere <codexhere@outlook.com>",
  "license": "MIT",
  "type": "module",
  "prettier": "@hangouthere/util/prettier",
  "engines": {
    "node": "=18"
  },
  "scripts": {
    "start": "echo \"You should use the included hh-util Docker Compose scripts located in './node_modules/.bin/hh-compose*' to start this project\"",
    "repl": "moleculer connect",
    "_start": "moleculer-runner"
  },
  "keywords": [
    "microservices",
    "moleculer"
  ],
  "devDependencies": {
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.2",
    "@types/tmi.js": "^1.8.3",
    "@types/uuid": "^9.0.2",
    "golden-hammer-shared": "^0.0.1-13",
    "moleculer-cli": "github:CodexHere/moleculer-cli#updateAllRemainingDeps",
    "moleculer-repl": "^0.7.3"
  },
  "dependencies": {
    "ioredis": "^5.3.2",
    "jaeger-client": "^3.19.0",
    "moleculer": "^0.14.29",
    "moleculer-db": "^0.8.23",
    "moleculer-db-adapter-mongo": "^0.4.17",
    "moleculer-io": "^2.2.0",
    "moleculer-web": "^0.10.5",
    "nats": "^2.15.1",
    "tmi.js": "^1.8.5",
    "uuid": "^9.0.0"
  },
  "__NEEDS_PORTING_TOVITEST__jest": {
    "resetModules": true,
    "resetMocks": true,
    "moduleNameMapper": {
      "@/(.*)$": "<rootDir>/$1"
    },
    "collectCoverageFrom": [
      "<rootDir>/services/**/*.{js,jsx}",
      "!<rootDir>/test/helpers/**",
      "!<rootDir>/node_modules/**"
    ]
  },
  "peerDependencies": {
    "golden-hammer-shared": "^0.0.1-1"
  }
}
